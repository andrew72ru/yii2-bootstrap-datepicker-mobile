{
    "name": "andrew72ru/yii2-bootstrap-datepicker-mobile",
    "description": "Mobile-first datepicker for Yii2 based on https://github.com/niftylettuce/bootstrap-datepicker-mobile",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","bootstrap","datepicker","mobile"],
    "license": "MIT",
    "authors": [
        {
            "name": "andrew72ru",
            "email": "andrew72ru@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/bootstrap-datepicker-mobile": "*"
    },
    "autoload": {
        "psr-4": {
            "andrew72ru\\datepicker\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
